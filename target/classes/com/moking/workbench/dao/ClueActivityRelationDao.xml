<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.moking.workbench.dao.ClueActivityRelationDao">
	
	<select id="getListById" resultType="com.moking.workbench.domain.ClueActivityRelation">
        select * from tbl_clue_activity_relation where clueId=#{cid}
    </select>

    <delete id="delete">
        delete from tbl_clue_activity_relation where clueId=#{cid}
    </delete>
</mapper>

































